        <td><%= politica_preventiva.pp9 %></td>
        <td><%= politica_preventiva.pp10 %></td>
        <td><%= politica_preventiva.pp11 %></td>
        <td><%= politica_preventiva.pp12 %></td>
        <td><%= link_to 'Show', politica_preventiva %></td>
        <td><%= link_to 'Edit', edit_politica_preventiva_path(politica_preventiva) %></td>
        <td><%= link_to 'Destroy', politica_preventiva, method: :delete, data: { confirm: 'Are you sure?' } %></td>
