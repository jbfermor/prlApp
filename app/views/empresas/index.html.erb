
<h1>Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Razon social</th>
      <th>Cif</th>
      <th>Direccion</th>
      <th>Poblacion</th>
      <th>Provincia</th>
      <th>Cp</th>
      <th>Telefono</th>
      <th>Email</th>
      <th>T fuera</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.razon_social %></td>
        <td><%= empresa.cif %></td>
        <td><%= empresa.direccion %></td>
        <td><%= empresa.poblacion %></td>
        <td><%= empresa.provincia %></td>
        <td><%= empresa.cp %></td>
        <td><%= empresa.telefono %></td>
        <td><%= empresa.email %></td>
        <td><%= empresa.t_fuera %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
